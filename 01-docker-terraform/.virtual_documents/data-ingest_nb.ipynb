


import pandas as pd
# import pyarrow.parquet as pq

pd.__file__





df = (
    pd.read_parquet(
        "green_tripdata_2025-11.parquet",
    )
    .drop(columns='ehail_fee')  # full of NaN values
    .astype({
        'RatecodeID': "Int64",
        'passenger_count': "Int64",
        'payment_type': "Int64",
        'trip_type': "Int64",
        'VendorID': "Int64",
        'PULocationID': "Int64",
        'DOLocationID': "Int64",
        # cbd_congestion_fee â†’ new
    })
)
display(df.info())


display(df.head(10))


display(df.tail(10))





from sqlalchemy import create_engine

POSTGRES_HOST = "localhost"
POSTGRES_PORT = 5433
POSTGRES_USER = "postgres"
POSTGRES_PASSWORD = "postgres"
POSTGRES_DB = "ny_taxi"
POSTGRES_TABLE = "green_taxi_data"

engine = create_engine(
    f"postgresql://{POSTGRES_USER}:{POSTGRES_PASSWORD}"
    f"@{POSTGRES_HOST}:{POSTGRES_PORT}/{POSTGRES_DB}"
)


df.head(n=0).to_sql(
    name=POSTGRES_TABLE,
    con=engine,
    if_exists="replace",
)
print("Table created\n")
print("Table scheme:")
print(pd.io.sql.get_schema(df, name=POSTGRES_TABLE, con=engine))


df.to_sql(
    name=POSTGRES_TABLE,
    con=engine,
    if_exists="append",
)
print("Table completed\n")





